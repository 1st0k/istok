{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            },
            "modules": "commonjs"
          }
        ],
        "@babel/preset-typescript",
        "@babel/preset-react"
      ],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "minify": false,
            "ssr": true,
            "namespace": "test"
          }
        ]
      ]
    }
  }
}